<h1>Registration of new user</h1>
<%=form_for( @rent_user, url: {action: :create})do |f|%>
    <%= f.label :name %>:
    <%= f.text_field :name %><br>
    <%= f.label :email %>:
    <%= f.text_field :email %><br>
    <%= f.label :Password %>:
    <%= f.password_field :password %><br>
    <%= submit_tag 'send'%>
<%end%>